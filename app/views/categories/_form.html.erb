<%= form_for(category) do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <%= f.hidden_field :parent_category_id, value: params[:parent_category_id] %>

  <%= f.hidden_field :user_id, value: current_user.id %>

  <div>
    <%= f.submit class: "btn"%>
  </div>
<% end %>
