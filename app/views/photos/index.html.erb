<% photos.each_with_index do |photo, i| %>
  <h3>
    Photo <%= i + 1 %>
  </h3>
  <div>Name: <%= photo.photo_with_type %></div>
  <div>Author: <%= photo.user_id %></div>
  <div>Category: <%= photo.category.name %></div>
  <div>Body: <%= photo.body %></div>
  <div><%= image_tag photo.avatar.url(:medium) %></div>
<% end %>
